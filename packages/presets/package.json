{
  "name": "@lumensuite/presets",
  "version": "0.17.5",
  "description": "Prebuilt LumenSuite editors and opt-in additional UI components.",
  "type": "module",
  "repository": "mdabidullah/lumensuite",
  "scripts": {
    "build": "tsc --build --verbose",
    "test:unit": "nx vite:test --browser.headless --run",
    "test:debug": "nx vite:test"
  },
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@lumensuite/affine-block-surface": "workspace:*",
    "@lumensuite/affine-shared": "workspace:*",
    "@lumensuite/block-std": "workspace:*",
    "@lumensuite/blocks": "workspace:*",
    "@lumensuite/global": "workspace:*",
    "@lumensuite/inline": "workspace:*",
    "@lumensuite/store": "workspace:*",
    "@floating-ui/dom": "catalog:",
    "@lit-labs/preact-signals": "catalog:",
    "@lottiefiles/dotlottie-wc": "catalog:",
    "@toeverything/theme": "catalog:",
    "lit": "catalog:",
    "zod": "catalog:"
  },
  "exports": {
    ".": "./src/index.ts",
    "./themes/*": "./themes/*"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "module": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "./themes/*": "./themes/*"
    }
  },
  "files": [
    "src",
    "dist",
    "themes",
    "!src/__tests__",
    "!dist/__tests__"
  ]
}
