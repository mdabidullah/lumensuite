{
  "name": "@lumensuite/store",
  "version": "0.17.5",
  "description": "LumenSuite data store built for general purpose state management.",
  "type": "module",
  "repository": "mdabidullah/lumensuite",
  "scripts": {
    "build": "tsc",
    "test:unit": "nx vite:test --run",
    "test:unit:coverage": "nx vite:test --run --coverage",
    "test:unit:ui": "nx vite:test --ui",
    "test": "pnpm test:unit"
  },
  "keywords": [],
  "author": "toeverything",
  "license": "MPL-2.0",
  "dependencies": {
    "@lumensuite/global": "workspace:*",
    "@lumensuite/inline": "workspace:*",
    "@lumensuite/sync": "workspace:*",
    "@preact/signals-core": "catalog:",
    "@types/flexsearch": "^0.7.6",
    "@types/lodash.ismatch": "^4.4.9",
    "flexsearch": "0.7.43",
    "lib0": "catalog:",
    "lodash.clonedeep": "^4.5.0",
    "lodash.ismatch": "^4.4.0",
    "lodash.merge": "^4.6.2",
    "minimatch": "catalog:",
    "nanoid": "catalog:",
    "y-protocols": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.merge": "^4.6.9",
    "lit": "catalog:",
    "yjs": "catalog:"
  },
  "peerDependencies": {
    "yjs": "catalog:"
  },
  "exports": {
    ".": "./src/index.ts",
    "./providers/*": "./src/providers/*.ts"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "import": "./dist/index.js"
      },
      "./shim.d.ts": "./shim.d.ts"
    }
  },
  "files": [
    "src",
    "dist",
    "providers",
    "!src/__tests__",
    "!dist/__tests__"
  ]
}
